<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script type="text/javascript">
	
	var expand = false;

	$(".interests").mouseenter(function() {
		$("#images").empty();
		$("#description").empty();
		$("#images").css("background-color","black");
		$(this).children("div").children().clone().css("position","relative").appendTo("#images");
		$("#images").children().css("float","left").css("border-style","none").css("margin","10px").fadeTo(0,1);
		$(this).children(":header").clone().css("color","white").css("font-size","50px").appendTo("#description");
	});
	$(".interests").click(function() {
		$("#images").empty();
		$("#description").empty();
	});

	$(function(){
		$("#drawing_img > img:gt(0)").hide();
		setInterval(function () {
			$("#drawing_img :first")
			.fadeOut(1000)
			.next()
			.fadeIn(1000)
			.end()
			.appendTo("#drawing_img");
		}, 2700);
	});
	$(function(){
		$("#music_img > img:gt(0)").hide();
		setInterval(function () {
			$("#music_img :first")
			.fadeOut(1000)
			.next()
			.fadeIn(1000)
			.end()
			.appendTo("#music_img");
		}, 2800);
	});
	$(function(){
		$("#cooking_img > img:gt(0)").hide();
		setInterval(function () {
			$("#cooking_img :first")
			.fadeOut(1000)
			.next()
			.fadeIn(1000)
			.end()
			.appendTo("#cooking_img");
		}, 2900);
	});
	$(function(){
		$("#knitting_img > img:gt(0)").hide();
		setInterval(function () {
			$("#knitting_img :first")
			.fadeOut(1000)
			.next()
			.fadeIn(1000)
			.end()
			.appendTo("#knitting_img");
		}, 3000);
	});

 </script>

